{
    "name": "genercodeltd/genercodeslim",
    "description": "A genercode wrapper for GenerCodeOrm and Slim",
    "authors": [
        {
            "name": "Suzanne Clifford",
            "email": "suzanne.clifford@presstojam.com"
        }
    ],
    "require": {
        "genercodeltd/genercodeorm": "^2.1",
        "slim/slim": ">=4.10",
        "slim/psr7": ">=1.5",
        "vlucas/phpdotenv": ">=5.4",
        "genercodeltd/genercodecmd": "^2.0.18",
        "illuminate/config": ">=8.83.1",
        "illuminate/http": ">=8.83.1",
        "illuminate/hashing": ">=8.83.1",
        "illuminate/cookie": ">=8.83.1",
        "symfony/psr-http-message-bridge": "^2.1",
        "zendframework/zend-diactoros": "^1.3",
        "nyholm/psr7-server": "^1.0",
        "laravel/sanctum": ">=2.15.1",
        "illuminate/auth": ">=8.83.1",
        "illuminate/session": ">=8.83.1",
        "illuminate/events": ">=8.83.1",
        "illuminate/log": ">=8.83.1",
        "illuminate/broadcasting": ">=8.83.1",
        "illuminate/cache": ">=8.83.1",
        "symfony/http-foundation": ">=5.4.16",
        "nyholm/psr7": "^1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload" : {
        "psr-4" : {
            "GenerCodeSlim\\" : "src/",
            "Illuminate\\Foundation\\" : "src/Illuminate/Foundation"
        }
    },
    "license" : "Apache-2.0"
}
